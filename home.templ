package main

templ MeetMenu(meets []*Meet) {
	<ul class="menu bg-base-200 w-56 rounded-box">
		<li class="menu-title">Closest Events</li>
		for _, meet := range meets {
			<li><a>{ meet.Name }</a></li>
		}
	</ul>
}

templ Home(meets []*Meet) {
	@Layout() {
		<div>
			@MeetMenu(meets)
		</div>
	}
}
